extends home
block main
  h1= title
  #user
    if (users.length > 0)
      for otherUser in users
        div.col-xs-6.col-md-4.col-lg-3
          ul.user-details.userBlock
            li
              a(href='/user/' + otherUser.username, class='listing-fonts users')
                img(class="smallImage", src="https://s3-us-west-2.amazonaws.com/clarkedbteer/"+otherUser.profilePicture)
                h4.user_name #{otherUser.firstName} #{otherUser.lastName}



